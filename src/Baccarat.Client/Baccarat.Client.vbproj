<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <OutputType>WinExe</OutputType>
    <RootNamespace>Baccarat.Client</RootNamespace>
    <AssemblyName>Baccarat.Client</AssemblyName>
    <OptionStrict>On</OptionStrict>
    <OptionInfer>On</OptionInfer>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Baccarat.Shared\Baccarat.Shared.vbproj" />
    <Reference Include="Experiment.TcpSocket">
      <HintPath>..\..\lib\Experiment.TcpSocket.dll</HintPath>
      <Private>true</Private>
    </Reference>
  </ItemGroup>
</Project>
